<main>
    <h1 class="welcome">Welcome to CSE Moters</h1>
    <%- messages() %>
    <div class="welcome-container">
        <div class="hero-container">
            <h2>DMC Delorean</h2>
            <p>3 Cup holders</p>
            <p>Superman doors</p>
            <p>Fuzzy dice!</p>
            <button class="own-today">Own Today</button>
